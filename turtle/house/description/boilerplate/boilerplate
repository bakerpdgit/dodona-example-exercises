import turtle


def house():
    # Draw a blue house
    turtle.color("blue")

    # Draw the base/square
    for i in range(_):
        turtle.forward(50)
        turtle.left(_)

    # Move up
    turtle.penup()
    turtle.left(90)
    turtle.forward(50)
    turtle.pendown()

    # Draw the roof/triangle
    turtle.right(_)
    turtle.forward(50)
    turtle.right(_)
    turtle.forward(50)


house()
